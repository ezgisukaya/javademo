import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
//kullanıcı adıyla ve sifreyle giris yapan kisiye hatalı giris veya basarılı giris yazısı gelcek

        String username,password ;
        Scanner inp = new Scanner(System.in);
        System.out.println("kullancı adınızı girin:");
        username= inp.nextLine();

        System.out.println("sifrenizi girin:");
        password= inp.nextLine();

        if(username.equals("ezgi") && password.equals("123")){
            System.out.println("Basarili giris");


        }else{
            System.out.println("hatalı giris ");
        }

    }
}
