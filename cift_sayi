import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        int a;
        Scanner input = new Scanner(System.in);
        System.out.print("üst sınır gir: ");
        a = input.nextInt();
        for(int k=0;k<=a;k=k+2){
            System.out.println(k);

        }






    }
}
